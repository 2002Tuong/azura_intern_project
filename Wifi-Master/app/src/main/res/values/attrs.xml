<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomRatingBar">
        <attr name="maxRating" format="integer" />
        <attr name="emptyStarDrawable" format="reference" />
        <attr name="filledStarDrawable" format="reference" />
        <attr name="emptyLastStarDrawable" format="reference" />
        <attr name="filledLastStarDrawable" format="reference" />
        <attr name="starSpacing" format="dimension" />
    </declare-styleable>

    <declare-styleable name="GaugeView">
        <attr name="gauge_backgroundColor" format="color" />
        <attr name="gauge_fillColor" format="color" />
        <attr name="gauge_strokeWidth" format="dimension" />
        <attr name="gauge_startAngle" format="integer" />
        <attr name="gauge_angles" format="integer" />
        <attr name="gauge_maxValue" format="integer" />
    </declare-styleable>
</resources>

<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">
            <gradient android:startColor="#8cb006"
                android:endColor="#bcde03"/>
            <size
                android:width="56dp"
                android:height="56dp" />
        </shape>
    </item>
    <item
        android:left="10dp"
        android:right="10dp">
        <shape android:shape="line">
            <stroke
                android:width="2dp"
                android:color="@color/black" />
        </shape>
    </item>
    <item
        android:left="10dp"
        android:right="10dp">
        <rotate
            android:fromDegrees="90">
            <shape android:shape="line">
                <stroke
                    android:width="2dp"
                    android:color="@color/black" />
            </shape>
        </rotate>
    </item>
</layer-list>